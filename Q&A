d = {"a":1, "b":2}
print(d.get("a", 0))      # 1
print(list(d.items()))    # [('a',1), ('b',2)]
print(list(d.keys()))     # ['a', 'b']
print(list(d.values()))   # [1, 2]

d.update({"c":3})         
print(d)                 # {'a':1, 'b':2, 'c':3}

d.pop("b")               
print(d)                 # {'a':1, 'c':3}

d.popitem()              
print(d)                 # {'a':1}

d.clear()                
print(d)                 # {}











